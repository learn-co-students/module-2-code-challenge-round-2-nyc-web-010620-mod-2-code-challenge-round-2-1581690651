<h1> Heroine Create page </h1>


<%= form_for @heroine_power do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <br>
   <%= f.label :super_name %>
   <%= f.text_field :super_name %>
   <br>
   <%= f.collection_select(:power_id, Power.all, :id, :name) %>
   <br>
   <%= f.submit %>
   <% end %>
